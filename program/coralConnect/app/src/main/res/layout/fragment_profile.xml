<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    tools:context=".fragment.ProfileFragment">

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="2dp"
        android:src="@drawable/orang"/>

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Since 2019"
        android:textStyle="italic"
        android:layout_toRightOf="@id/imageView12"
        android:layout_below="@id/textView9"/>

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginTop="4dp"
        android:paddingRight="2dp"
        android:layout_toRightOf="@id/imageView12"
        android:layout_below="@id/textView6"
        android:text="Penjaga Pantai" />
    <TextView
        android:id="@+id/textView6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:text="Udinagara"
        android:textSize="20dp"
        android:layout_toRightOf="@id/imageView12"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Terkadang, yang paling berharga di balik biru tak terbatas adalah kehidupan tak terlihat yang kita lindungi di bawah permukaan. Jadilah penjaga terumbu karang, pelindung cinta laut yang membentuk masa depan kita. "
        android:layout_toRightOf="@id/imageView12"
        android:layout_below="@id/textView10"/>
</RelativeLayout>
